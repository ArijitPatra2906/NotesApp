{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react"
  },
  "include": [
    "src/**/*",
    "src/types/**/*"  // Make sure this is included
  ]
}
